{
  "name": "mopidy-botrock",
  "version": "0.1.0",
  "description": "Mopidy HTTP interface",
  "repository": "https://github.com/bhavens17/Mopidy-BotRock",
  "author": "Brad Havens <bhavens@hawk.iit.edu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhavens17/Mopidy-BotRock/issues"
  },
  "main": "app.js",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "copy-dir": "^0.3.0",
    "css-loader": "^0.28.7",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "flux": "^3.1.3",
    "jquery": "^3.2.1",
    "mopidy": "^0.5.0",
    "node-sass": "^4.7.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-fontawesome": "*",
    "react-ga": "^2.3.4",
    "react-lazyload": "2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "3.7.2",
    "redux-devtools": "3.4.0",
    "redux-thunk": "2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-strip": "0.1.0"
  },
  "scripts": {
    "build": "node build_tools/build.js",
    "dev": "node build_tools/build.js && NODE_ENV=development&& webpack --watch",
    "dev windows": "set NODE_ENV=development&& node build_tools/build.js && webpack --watch",
    "prod": "node build_tools/build.js && NODE_ENV=development&& webpack && NODE_ENV=production webpack --watch",
    "prod windows": "node build_tools/build.js && set NODE_ENV=development&& webpack && set NODE_ENV=production&& webpack --watch",
    "release": "node build_tools/build.js && NODE_ENV=production&& webpack && NODE_ENV=development webpack && python setup.py sdist upload -r pypi && build_tools/release.sh",
    "release windows": "node build_tools/build.js && NODE_ENV=production webpack && NODE_ENV=development webpack && python setup.py sdist upload -r pypi && build_tools/release.sh"
  }
}
